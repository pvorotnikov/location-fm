<Window x:Class="location_fm.StatusDemos.HomeTimelineWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:location_fm.StatusDemos"
        mc:Ignorable="d"
        Title="HomeTimelineWindow" Height="300" Width="300" Loaded="Window_Loaded">
    <Grid>
        <DataGrid HorizontalAlignment="Stretch" Margin="5" VerticalAlignment="Stretch" ItemsSource="{Binding Tweets}"/>
    </Grid>
</Window>
