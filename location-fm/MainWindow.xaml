<Window x:Class="location_fm.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:location_fm"
        mc:Ignorable="d"
        Title="Location.FM" Height="350" Width="525" Loaded="Window_Loaded">
    <Grid>
        <DockPanel LastChildFill="False" >
            <Menu DockPanel.Dock="Top">
                <MenuItem BorderThickness="5" Header="Status">
                    <MenuItem BorderThickness="5" Header="Home Timeline" Tag="location_fm.StatusDemos.HomeTimelineWindow" Click="MenuItem_Click"/>
                </MenuItem>
            </Menu>
        </DockPanel>
    </Grid>
</Window>
